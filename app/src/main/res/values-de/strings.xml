    <string name="app_name">Fingerprint Quick Action</string>

    <string name="xposed_description">Schnellaktionen durch Antippen und Wischen auf dem Fingerabdruck-Leser auslösen. Die Aktivierung des Xposed-Moduls verbessert die Stabilität.</string>

    <!-- FPQAService -->
    <string name="toast_notify_on_error">Fingerprint Quick Action: Fehler\n%1$s</string>
    <string name="notification_content_text">Fingerprint Quick Action läuft</string>
    <string name="notification_content_text_retry">Hier klicken, um es erneut zu probieren. </string>
